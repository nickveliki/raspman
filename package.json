    "react-html-parser": "^2.0.2",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "style-loader": "^1.1.3",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
